<spyglassConfiguration>
   <packetReader class="de.uniluebeck.itm.spyglass.packet.SimplePacketReader">
      <gateway class="de.uniluebeck.itm.spyglass.gateway.FileReaderGateway">
         <file>source.packet.txt</file>
      </gateway>
   </packetReader>
   <canvas class="de.uniluebeck.itm.spyglass.drawing.Canvas2D"/>
   <instances>
      <plugins class="java.util.ArrayList">
         <plugin class="de.bsi.flegsens.FlegsensNodePainterPlugin" isActive="true"/>
         <plugin class="de.uniluebeck.itm.spyglass.plugin.SimpleNodePainterPlugin" isActive="true">
            <parameters>
               <extendedInformation>
                  <isActive nodeID="1">false</isActive>
                  <isActive nodeID="2">true</isActive>
                  <isActive nodeID="3">false</isActive>
               </extendedInformation>
               <lineColorRGB length="3">
                  <int>0</int>
                  <int>0</int>
                  <int>255</int>
               </lineColorRGB>
               <lineWidth>1</lineWidth>
               <stringFormatters/>
               <isActive>true</isActive>
               <isVisible>false</isVisible>
               <name>SimpleNodePainterTest</name>
               <semanticTypes length="2">
                  <int>0</int>
                  <int>1</int>
               </semanticTypes>
            </parameters>
         </plugin>
         <plugin class="de.uniluebeck.itm.spyglass.plugin.ImagePainterPlugin" isActive="true">
            <parameters>
               <image>images/icons/addMarker.png</image>
               <isActive>true</isActive>
               <isVisible>true</isVisible>
               <name>default</name>
            </parameters>
         </plugin>
      </plugins>
   </instances>
   <nodePositioner class="de.bsi.flegsens.RandomNodePositioner" isActive="true"/>
   <generalSettings>
      <showRuler>true</showRuler>
      <metrics>
         <unit>m</unit>
         <abs2metricFactorX>1.0</abs2metricFactorX>
         <abs2metricFactorY>2.0</abs2metricFactorY>
         <abs2metricOffsetX>3.0</abs2metricOffsetX>
         <abs2metricOffsetY>4.0</abs2metricOffsetY>
         <lockAbs2metricFactor>true</lockAbs2metricFactor>
      </metrics>
      <timeUnit>s</timeUnit>
      <timeScale>1.0</timeScale>
      <packetDeliveryDelay>400</packetDeliveryDelay>
      <packetDeliveryInitialDelay>1000</packetDeliveryInitialDelay>
      <framesPerSecond>25</framesPerSecond>
   </generalSettings>
   <defaults>
      <plugin class="de.bsi.flegsens.FlegsensNodePainterPlugin" isActive="true"/>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.GridPainterPlugin" isActive="true">
         <parameters>
            <gridElementHeight>1</gridElementHeight>
            <gridElementWidth>1</gridElementWidth>
            <gridLowerLeftPoint x="0" y="0" z="0"/>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1.0</lineWidth>
            <lockGridElementsSquare>true</lockGridElementsSquare>
            <lockNumberOfRowsNCols>true</lockNumberOfRowsNCols>
            <numCols>1</numCols>
            <numRows>1</numRows>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.ImagePainterPlugin" isActive="true">
         <parameters>
            <image>images/icons/brokenImageLink.png</image>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.MapPainterPlugin" isActive="true">
         <parameters>
            <framePointDefaultValue>3.0</framePointDefaultValue>
            <gridElementHeight>1</gridElementHeight>
            <gridElementWidth>1</gridElementWidth>
            <gridLowerLeftPoint x="0" y="0" z="0"/>
            <lockGridElementSquare>true</lockGridElementSquare>
            <lockNumberOfRowsNCols>true</lockNumberOfRowsNCols>
            <maxColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </maxColorRGB>
            <maxValue>1.0</maxValue>
            <minColorRGB length="3">
               <int>255</int>
               <int>255</int>
               <int>255</int>
            </minColorRGB>
            <minValue>0.0</minValue>
            <numCols>1</numCols>
            <numFramePointsHorizontal>3</numFramePointsHorizontal>
            <numFramePointsVertical>3</numFramePointsVertical>
            <numRows>1</numRows>
            <refreshInterval>10</refreshInterval>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.ObjectPainterPlugin" isActive="true">
         <parameters>
            <image>images/icons/brokenImageLink.png</image>
            <size width="10" height="10" lineWidth="1">
               <colorR>200</colorR>
               <colorG>0</colorG>
               <colorB>0</colorB>
               <bgColorR>255</bgColorR>
               <bgColorG>255</bgColorG>
               <bgColorB>255</bgColorB>
            </size>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.SimpleGlobalInformationPlugin" isActive="true">
         <parameters>
            <semanticTypes4Neighborhoods class="java.util.LinkedList"/>
            <showNodeDegree>true</showNodeDegree>
            <showNumNodes>true</showNumNodes>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.SimpleNodePainterPlugin" isActive="true">
         <parameters>
            <extendedInformation/>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1</lineWidth>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.PositionPacketNodePositionerPlugin" isActive="true">
         <parameters>
            <timeToLive>10</timeToLive>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.SpringEmbedderPositionerPlugin" isActive="true">
         <parameters>
            <edgeSemanticTypes class="java.util.LinkedList"/>
            <timeToLive>10</timeToLive>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.bsi.flegsens.RandomNodePositioner" isActive="true"/>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.LinePainterPlugin" isActive="true">
         <parameters>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1</lineWidth>
            <stringFormatters/>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.VectorSequencePainterPlugin" isActive="true">
         <parameters>
            <dimension>2</dimension>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1</lineWidth>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
   </defaults>
</spyglassConfiguration>