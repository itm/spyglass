<spyglassConfiguration>
   <packetReader class="de.uniluebeck.itm.spyglass.packet.ComplexPacketReader">
      <gateway class="de.uniluebeck.itm.spyglass.gateway.FileReaderGateway">
         <file>generator.txt</file>
      </gateway>
      <delayMillies>1000</delayMillies>
   </packetReader>
   <instances>
      <plugins class="java.util.ArrayList">
         <plugin class="de.bsi.flegsens.FlegsensNodePainterPlugin"/>
         <plugin class="de.uniluebeck.itm.spyglass.plugin.simplenodepainter.SimpleNodePainterPlugin">
            <parameters>
               <extendedInformation>
                  <isActive nodeID="1">false</isActive>
                  <isActive nodeID="2">true</isActive>
                  <isActive nodeID="3">false</isActive>
               </extendedInformation>
               <isExtendedDefaultValue>false</isExtendedDefaultValue>
               <lineColorRGB length="3">
                  <int>0</int>
                  <int>0</int>
                  <int>255</int>
               </lineColorRGB>
               <lineWidth>1</lineWidth>
               <stringFormatters/>
               <isActive>true</isActive>
               <isVisible>false</isVisible>
               <name>SimpleNodePainterTest</name>
               <semanticTypes length="2">
                  <int>0</int>
                  <int>1</int>
               </semanticTypes>
            </parameters>
         </plugin>
         <plugin class="de.uniluebeck.itm.spyglass.plugin.imagepainter.ImagePainterPlugin">
            <parameters>
               <image>images/icons/addMarker.png</image>
               <isActive>true</isActive>
               <isVisible>true</isVisible>
               <name>default</name>
            </parameters>
         </plugin>
      </plugins>
   </instances>
   <nodePositioner class="de.uniluebeck.itm.spyglass.plugin.positionpacketnodepositioner.PositionPacketNodePositionerPlugin">
      <parameters>
         <timeToLive>60</timeToLive>
         <isActive>true</isActive>
         <isVisible>true</isVisible>
         <name>default</name>
      </parameters>
   </nodePositioner>
   <generalSettings>
      <showRuler>true</showRuler>
      <metrics>
         <unit>m</unit>
         <abs2metricFactorX>1.0</abs2metricFactorX>
         <abs2metricFactorY>2.0</abs2metricFactorY>
         <abs2metricOffsetX>3.0</abs2metricOffsetX>
         <abs2metricOffsetY>4.0</abs2metricOffsetY>
         <lockAbs2metricFactor>true</lockAbs2metricFactor>
      </metrics>
      <timeUnit>s</timeUnit>
      <timeScale>1.0</timeScale>
      <packetDeliveryInitialDelay>1000</packetDeliveryInitialDelay>
      <framesPerSecond>25</framesPerSecond>
   </generalSettings>
   <defaults>
      <plugin class="de.bsi.flegsens.FlegsensNodePainterPlugin"/>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.gridpainter.GridPainterPlugin">
         <parameters>
            <gridElementHeight>1</gridElementHeight>
            <gridElementWidth>1</gridElementWidth>
            <gridLowerLeftPoint x="0" y="0" z="0"/>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1.0</lineWidth>
            <lockGridElementsSquare>true</lockGridElementsSquare>
            <lockNumberOfRowsNCols>true</lockNumberOfRowsNCols>
            <numCols>1</numCols>
            <numRows>1</numRows>
            <isActive>true</isActive>
            <isVisible>true</isVisible>            
            <timeout>0</timeout>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.imagepainter.ImagePainterPlugin">
         <parameters>
            <image>images/icons/brokenImageLink.png</image>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <timeout>0</timeout>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.mappainter.MapPainterPlugin">
         <parameters>
            <framePointDefaultValue>3.0</framePointDefaultValue>
            <gridElementHeight>1</gridElementHeight>
            <gridElementWidth>1</gridElementWidth>
            <gridLowerLeftPoint x="0" y="0" z="0"/>
            <lockGridElementSquare>true</lockGridElementSquare>
            <lockNumberOfRowsNCols>true</lockNumberOfRowsNCols>
            <maxColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </maxColorRGB>
            <maxValue>1.0</maxValue>
            <minColorRGB length="3">
               <int>255</int>
               <int>255</int>
               <int>255</int>
            </minColorRGB>
            <minValue>0.0</minValue>
            <numCols>1</numCols>
            <numFramePointsHorizontal>3</numFramePointsHorizontal>
            <numFramePointsVertical>3</numFramePointsVertical>
            <numRows>1</numRows>
            <refreshInterval>10</refreshInterval>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <timeout>0</timeout>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.objectpainter.ObjectPainterPlugin">
         <parameters>
            <image>images/icons/brokenImageLink.png</image>
            <size height="1" width="1">
               <upperLeft x="0" y="0" z="0"/>
            </size>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.simpleglobalinformation.SimpleGlobalInformationPlugin">
         <parameters>
            <semanticTypes4Neighborhoods class="java.util.LinkedList"/>
            <showNodeDegree>true</showNodeDegree>
            <showNumNodes>true</showNumNodes>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.simplenodepainter.SimpleNodePainterPlugin">
         <parameters>
            <extendedInformation/>
            <isExtendedDefaultValue>false</isExtendedDefaultValue>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1</lineWidth>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.positionpacketnodepositioner.PositionPacketNodePositionerPlugin">
         <parameters>
            <timeToLive>10</timeToLive>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.springembedderpositioner.SpringEmbedderPositionerPlugin">
         <parameters>
            <edgeSemanticTypes class="java.util.LinkedList"/>
            <timeToLive>10</timeToLive>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.bsi.flegsens.RandomNodePositioner"/>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.linepainter.LinePainterPlugin">
         <parameters>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1</lineWidth>
            <stringFormatters/>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
      <plugin class="de.uniluebeck.itm.spyglass.plugin.vectorsequencepainter.VectorSequencePainterPlugin">
         <parameters>
            <dimension>2</dimension>
            <lineColorRGB length="3">
               <int>0</int>
               <int>0</int>
               <int>0</int>
            </lineColorRGB>
            <lineWidth>1</lineWidth>
            <isActive>true</isActive>
            <isVisible>true</isVisible>
            <name>default</name>
         </parameters>
      </plugin>
   </defaults>
   <drawingArea>

   </drawingArea>
</spyglassConfiguration>